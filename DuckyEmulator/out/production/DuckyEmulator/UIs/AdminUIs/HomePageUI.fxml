<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UIControllers.AdminUIsControllers.HomePageUIController">
   <left>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <VBox alignment="CENTER" spacing="40.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Menu">
                     <font>
                        <Font name="Proxima Nova Bold" size="30.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnMenuHome" mnemonicParsing="false" onAction="#btnMenuHomeClick" text="Home">
                     <font>
                        <Font name="Proxima Nova" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnMenuQuestion" mnemonicParsing="false" onAction="#btnMenuQuestionClick" text="Questions">
                     <font>
                        <Font name="Proxima Nova" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnMenuTopicClass" mnemonicParsing="false" onAction="#btnMenuTopicClassClick" text="Topics/Classifications">
                     <font>
                        <Font name="Proxima Nova" size="18.0" />
                     </font>
                  </Button>
                  <Line endX="100.0" rotate="90.0" scaleX="2.0" scaleY="5.0" startX="-100.0" translateX="110.0" translateY="-150.0" />
               </children>
               <BorderPane.margin>
                  <Insets left="5.0" right="20.0" />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
   </left>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <top>
            <HBox alignment="CENTER" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="30.0" />
               </BorderPane.margin>
            </HBox>
         </top>
         <center>
            <HBox alignment="CENTER" spacing="30.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnHomeQuestion" mnemonicParsing="false" onAction="#btnHomeQuestionClick" text="Questions">
                     <font>
                        <Font name="Proxima Nova" size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnHomeTopicClass" mnemonicParsing="false" onAction="#btnHomeTopicClassClick" text="Topics/Classifications">
                     <font>
                        <Font name="Proxima Nova" size="30.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="30.0" right="30.0" />
               </padding>
            </HBox>
         </center>
      </BorderPane>
   </center>
   <top>
      <Label text="DuckyEmulator Admin Console" BorderPane.alignment="CENTER">
         <font>
            <Font name="Proxima Nova Bold" size="30.0" />
         </font>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </Label>
   </top>
</BorderPane>
